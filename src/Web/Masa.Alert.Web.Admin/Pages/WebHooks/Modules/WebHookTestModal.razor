@inherits AdminCompontentBase
<PModal @bind-Value="_visible"
        Persistent
        Title="@T("TestWebHook")"
        HideCancelAction
        Width="398"
        FormModel="_model"
        HideTitleDivider
        HideActionsDivider
        HeaderClass="h6 emphasis2--text pa-6 pb-0"
        ActionsClass="px-6 pt-4 pb-9"
        OnSave="HandleOkAsync"
        OnCancel="HandleCancel">
    <ChildContent>
        <SUserAutoComplete @bind-Value="_model.UserId" />
    </ChildContent>
    <SaveContent Context="save">
        <SButton Class="full-width btn" Medium OnClick="save.Click">@T("Test")</SButton>
    </SaveContent>
</PModal>